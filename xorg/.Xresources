! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
! 
! 
! -- Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #CAA9FA
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6

! Use extended color scheme
rofi.color-enabled:                  true
rofi.fake-transparency:                  true
rofi.color-window:    #282a36, #282a36, #44475a
rofi.color-normal:    #282a36, #f8f8f2, #282a36, #f8f8f2, #282a36
rofi.color-active:    #282a36, #f1fa8c, #282a36, #f1fa8c, #282a36
rofi.color-urgent:    #282a36, #ff5555, #282a36, #ff5555, #282a36
! Separator style (none, dash, solid)
rofi.separator-style:                none
!rofi.font:            Comic\ Sans\ MS 13
!rofi.font:            Comic\ Neue\ Bold 13
!rofi.font:            Comic\ Neue, FontAwesome Bold 13
!rofi.font:            Comic\ Neue, FontAwesome 13
rofi.font:            Inziu\ Iosevka\ Slab\ SC, FontAwesome 13
rofi.terminal:        st
rofi.combi-modi:            window,drun
rofi.kb-cancel:                      Escape,Control+g,Control+bracketleft,Super+d
